                            <!-- node_id = NodeId(id_str='cm_1') -->
                            <!-- metric_spec = MetricSpec(element_name='bookings') -->
                            <AggregateSimpleMetricInputsNode>
                                <!-- description = 'Aggregate Inputs for Simple Metrics' -->
                                <!-- node_id = NodeId(id_str='am_1') -->
